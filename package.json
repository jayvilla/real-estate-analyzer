  "scripts": {
    "dev": "NX_DAEMON=falsenx run-many -t serve dev --projects=api,web --parallel",
    "dev:api": "NX_DAEMON=false nx serve api",
    "dev:web": "NX_DAEMON=false nx dev web",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "db:migrate": "PGPASSWORD=postgres psql -h localhost -p 5433 -U postgres -d real_estate_analyzer -f apps/api/src/migrations/init.sql",
    "db:migrate:update-enum": "PGPASSWORD=postgres psql -h localhost -p 5433 -U postgres -d real_estate_analyzer -f apps/api/src/migrations/update-property-type-enum.sql",
    "db:migrate:deals": "PGPASSWORD=postgres psql -h localhost -p 5433 -U postgres -d real_estate_analyzer -f apps/api/src/migrations/create-deals-table.sql",
    "test:logging": "bash apps/api/test-logging.sh",
    "test:errors": "bash apps/api/test-error-standardization.sh",
    "test:valuation": "bash apps/api/test-valuation.sh",
    "test:events": "bash apps/api/test-events.sh"
  },
